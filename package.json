  "author": "The Beaverhead <http://thebeaverhead.com>",
  "contributors": [
    {
      "name": "Olaf Meller",
      "email": "meller.olaf@gmail.com"
    }
  ],
  "jest": {
    "setupFiles": [
      "./jestConfig.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
